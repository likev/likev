<Window x:Class="ContourTracker01.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="480" Width="640">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition MinHeight="20" Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        <ToolBarTray Grid.Row="0">
            <ToolBar>
                <Button Name="_openFileButton" Click="_openFileButton_Click">
                    <Image Source="openHS.png" />
                </Button>
                <Button Name="_editContourLevelButton" Click="_editContourLevelButton_Click" IsEnabled="False">
                    <Image Source="EditInformationHS.BMP"/>
                </Button>
            </ToolBar>
        </ToolBarTray>
        
        <Canvas Grid.Row="1" Name="_canvas"/>
    </Grid>
</Window>
